{
    "name": "ensi/laravel-phprdkafka-producer",
    "description": "Opiniated High Level producer for laravel-phprdkafka",
    "keywords": [
        "ensi",
        "laravel",
        "kafka"
    ],
    "homepage": "https://github.com/ensi-platform/laravel-phprdkafka-producer",
    "license": "MIT",
    "authors": [
        {
            "name": "Nekrasov Ilya",
            "email": "nekrasov@greensight.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-rdkafka": "*",
        "illuminate/contracts": "^8.37 || ^9",
        "illuminate/pipeline": "^8.37 || ^9",
        "illuminate/support": "^8.37 || ^9",
        "ensi/laravel-phprdkafka": "^0.2"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.2",
        "nunomaduro/collision": "^5.3 || ^6",
        "orchestra/testbench": "^6.15 || ^7",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "php-parallel-lint/php-var-dump-check": "^0.5.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Ensi\\LaravelPhpRdKafkaProducer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ensi\\LaravelPhpRdKafkaProducer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --config .php-cs-fixer.php",
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ensi\\LaravelPhpRdKafkaProducer\\LaravelPhpRdKafkaProducerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
