{
    "name": "greensight/laravel-phprdkafka-producer",
    "description": "Opiniated High Level producer for laravel-phprdkafka",
    "keywords": [
        "greensight",
        "laravel",
        "kafka"
    ],
    "homepage": "https://github.com/greensight/laravel-phprdkafka-producer",
    "license": "MIT",
    "authors": [
        {
            "name": "arrilot",
            "email": "nekrasov@greensight.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-rdkafka": "*",
        "greensight/laravel-phprdkafka": "^0.1.4"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Greensight\\LaravelPhpRdKafkaProducer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greensight\\LaravelPhpRdKafkaProducer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
